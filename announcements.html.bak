    <style>*,
*::after,
*::before {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  --bg-color: #f4f4f4;
  --dark-color: #333;
  --light-color: #666;
  --pri-dark-color: hsl(13, 80%, 12%);
  --pri-light-color: hsl(40, 90%, 65%);
}
@font-face {
  font-family: "notoSans";
  src: url("undefined") format("truetype");
  font-weight: normal;
}
body {
  font-family: "Open Sans", sans-serif;
  font-size: 18px;
  line-height: 1.5;
  color: var(--dark-color);
  background: var(--bg-color);

  word-break: keep-all;
}

body > p {
  padding-bottom: 1rem;
}

.top {
  margin: auto;
  max-width: 55rem;
  padding: 2em 1em;

  display: flex;
  flex-direction: column;

  justify-content: center;
}

.title {
  display: block;
  font-weight: 800;
  font-size: 3.5rem;
  width: fit-content;
  line-height: 1;
}

.top div {
  width: fit-content;
  padding-inline: 0.25rem;
}

.top p {
  font-size: 0.875rem;
}

h1 {
  padding-block: 1em 0.5em;
  width: fit-content;

  font-size: 2.25rem;
  font-weight: 600;
}
h1::after {
  content: "";
  display: block;
  margin-top: 0.25em;

  width: 100%;
  height: 3px;

  background: var(--dark-color);
}

h2 {
  padding-block: 1em;
  width: fit-content;

  font-size: 1.5rem;
  font-weight: 600;
}

h2::after {
  content: "";
  display: block;

  margin-top: 0.25em;

  width: 100%;
  height: 3px;

  background: var(--dark-color);
}

h2:first-of-type {
  padding-top: 0;
}

header {
  position: sticky;
  top: 0px;
  background-color: var(--bg-color);
  z-index: 100;
}

nav {
  margin-inline: auto;
  max-width: 55rem;
}

a {
  text-decoration: none;
  color: var(--dark-color);
}

a:hover {
  color: var(--light-color);
}

nav ul {
  position: relative;
  display: flex;
  flex-direction: row;
  list-style-type: none;
  flex-wrap: wrap;
  border-bottom: 3px solid var(--dark-color);
}

nav ul a {
  flex-shrink: 0;
  padding-inline: 1.25rem;
  padding-block: 0.75rem;
  font-weight: 400;
  transition: background-color 0.2s ease-in-out;
}

nav ul a > a {
  transition: color 0.2s ease-in-out;
}

nav ul a:hover {
  background-color: var(--light-color);
}

nav ul a:hover {
  color: white;
}

.active {
  font-weight: 900;
  background-color: var(--dark-color);
}

.active {
  color: white;
}

main {
  padding-inline: 1rem;
  margin-inline: auto;
  max-width: 55rem;
  min-height: 50vh;
}

section {
  padding-inline: 0.5rem;
}

.image-list {
  padding-top: 3rem;
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 0.5rem;
  
}

.image-list div {
  border: #333 3px solid;
  border-radius: 0.5rem;
  overflow: hidden;
}

.image-list img {
  object-fit: cover;
  width: 100%;
  vertical-align: bottom;
  height: 80px;
}

footer {
  max-width: 55rem;
  margin-inline: auto;
  padding: 1rem;
  border-top: var(--dark-color) solid 3px;
  padding-block: 2rem;
  margin-top: 4rem;
  font-size: 1rem;
}

.grid-col-2 {
  display: grid;
  grid-template-columns: auto 1fr;
  gap: 0.5rem;
}

.grid-head {
  font-weight: 800;
  text-align: end;
}

.list li {
  list-style-type: disc;
  margin-inline: 1rem;
}

.button {
  padding: 0.75rem 1rem;
  width: fit-content;
  border-radius: 0.5em;

  display: flex;
  justify-content: center;
  align-items: center;
  gap: 0.5em;

  background-color: var(--bg-color);
  color: var(--dark-color);
  font-weight: bold;

  border: 3px solid var(--dark-color);
  transition: background-color 300ms ease-in-out, color 300ms ease-in-out;
}

.button:hover {
  color: #eee;
  background-color: var(--dark-color);
}

.button-important {
  background-color: var(--pri-light-color);
  border-color: var(--pri-dark-color);
  color: var(--pri-dark-color);
}

.button-important:hover {
  background-color: var(--pri-dark-color);
  color: #eee;
}

.file-list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  gap: 0.5rem;
}

.word-file {
  color: hsl(230, 80%, 40%);
  border-color: hsl(230, 70%, 60%);
}

.word-file:hover {
  color: white;
  border-color: hsl(230, 70%, 60%);
  background-color: hsl(230, 70%, 40%);
}

.hwp-file {
  color: hsl(195, 80%, 40%);
  border-color: hsl(195, 70%, 50%);
}

.hwp-file:hover {
  color: white;
  border-color: hsl(195, 70%, 60%);
  background-color: hsl(195, 80%, 50%);
}

.flex-row {
  padding-top: 0.5rem;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  gap: 0.5rem;
}

